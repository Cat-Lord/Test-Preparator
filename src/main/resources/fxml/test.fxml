<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="580.0" prefWidth="1106.0" spacing="20.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="questionLabel" text="Question">
         <font>
            <Font size="22.0" />
         </font></Label>
      <VBox fx:id="answersBox" prefHeight="365.0" prefWidth="246.0" spacing="24.0" />
      <StackPane prefHeight="150.0" prefWidth="200.0">
         <children>
            <Button fx:id="answerButton" onAction="#answer" text="Check Answer" />
            <Button fx:id="nextButton" mnemonicParsing="false" onAction="#next" prefHeight="26.0" prefWidth="110.0" text="Next" />
            <Button fx:id="goToMenuButton" mnemonicParsing="false" onAction="#goToMenu" prefHeight="64.0" prefWidth="88.0" text="Menu" />
         </children>
      </StackPane>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
